<vibelyApp>:
    screen_manager: screen_managerA
    ScreenManager:
        id:screen_managerA
        Screen:
            id: dash_screen
            name: "dashScreen"
            MDBoxLayout:
                orientation: "vertical"
                size: root.size
                MDTopAppBar:
                    id: toolbar
                    title: "Navigation Bar"
                    left_action_items: [["menu"]]
                    right_action_items: [["dots-vertical", lambda x: root.dropdown(x)]]

                MDBottomNavigation:
                    panel_color: app.theme_cls.primary_color
                    text_color_active: app.theme_cls.primary_dark
                    text_color_normal: 1,1,1,1
                    use_text: False
                    transition_duration: .2 #animate a fade in screen switch
                    MDBottomNavigationItem:
                        name: 'screen 1'
                        icon: 'magnify'
                        #badge_icon: "numeric-10"
                        
                        MDLabel:
                            text: 'GALAN/ VILLARUEL TAB'
                            halign: 'center'
                            font_size: '55sp'
                            theme_text_color: "Custom"
                            text_color: app.theme_cls.primary_color
                    MDBottomNavigationItem:
                        name: 'screen 2'
                        icon: 'head-snowflake-outline'
                        #badge_icon: "numeric-10"
                        
                        MDLabel:
                            text: 'ALL()'
                            halign: 'center'
                            font_size: '55sp'
                            theme_text_color: "Custom"
                            text_color: app.theme_cls.primary_color

                    MDBottomNavigationItem:
                        name: 'screen 3'
                
                        icon: 'home-outline'
                        
                        #badge_icon: "numeric-10"
                        MDLabel:
